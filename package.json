{
  "name": "merkle-tree-stream",
  "version": "4.0.0",
  "description": "A stream that generates a merkle tree based on the incoming data.",
  "main": "./index.js",
  "types": "./index.d.ts",
  "dependencies": {
    "@types/streamx": "^2.9.1",
    "flat-tree": "^1.8.0",
    "streamx": "^2.11.1"
  },
  "devDependencies": {
    "c8": "^7.11.0",
    "dtslint": "^4.2.1",
    "standard": "^16.0.3",
    "tape": "^5.3.1",
    "typescript": "^4.5.5"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.js"
    },
    "./generator": {
      "types": "./generator.d.ts",
      "require": "./generator.js"
    },
    "./generator.js": {
      "types": "./generator.d.ts",
      "require": "./generator.js"
    },
    "./util": {
      "types": "./util.d.ts",
      "require": "./util.js"
    },
    "./util.js": {
      "types": "./util.d.ts",
      "require": "./util.js"
    },
    "./types": {
      "types": "./types.d.ts",
      "require": "./types.js"
    },
    "./types.js": {
      "types": "./types.d.ts",
      "require": "./types.js"
    }
  },
  "scripts": {
    "test": "npm run lint && npm run coverage",
    "lint": "standard && dtslint --localTs node_modules/typescript/lib types",
    "coverage": "c8 npm run unit",
    "unit": "tape test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mafintosh/merkle-tree-stream.git"
  },
  "author": "Mathias Buus (@mafintosh)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mafintosh/merkle-tree-stream/issues"
  },
  "homepage": "https://github.com/mafintosh/merkle-tree-stream"
}
